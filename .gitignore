# Laravel Minimal .gitignore

# Environment files (never commit secrets!)
.env
.env.production
.env.staging
.env.example.local

# Laravel storage and cache
/storage/*.key
/storage/framework/cache/
/storage/framework/sessions/
/storage/framework/views/
/storage/logs/

# Application cache
/bootstrap/cache/

# Composer dependencies
/vendor/

# Node.js dependencies
/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Built assets
/public/hot
/public/storage
/public/css/
/public/js/

# IDE files
.idea/
.phpstorm.meta.php
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Local backup files
*.backup
*.sqlite
*.sqlite-journal

# Testing
.phpunit.result.cache
